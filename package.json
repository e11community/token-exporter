{
  "name": "token-exporter",
  "version": "1.0.0",
  "description": "Find NPM auth tokens in HOME/.npmrc and exports them as environments variables",
  "main": "lib.js",
  "bin": {
    "export-tokens": "./export-tokens.js"
  },
  "scripts": {
    "test": "jest",
    "atest": "jest ${0}",
    "test:coverage": "jest --coverage",
    "lint": "gts lint",
    "clean": "gts clean",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "fix": "gts fix",
    "version": "yarn version",
    "publish": "yarn npm publish",
    "prepack": "yarn run clean && yarn run build",
    "pretest": "yarn run build",
    "posttest": "yarn run lint"
  },
  "keywords": [
    "auth",
    "token"
  ],
  "author": "Todd Trimmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/e11community/token-exporter/issues"
  },
  "repository": "https://github.com/e11community/token-exporter",
  "homepage": "https://github.com/e11community/token-exporter#readme",
  "devDependencies": {
    "gts": "^3.1.0",
    "ts-node": "^10.1.0"
  },
  "dependencies": {
    "@types/node": "^16.4.10",
    "typescript": "^4.3.5"
  }
}
