{
  "name": "@e11community/token-exporter",
  "version": "3.1.0",
  "description": "Find NPM auth tokens in HOME/.npmrc and export them as environment variables",
  "main": "./build/src/lib.js",
  "types": "./build/src/lib.d.ts",
  "files": [
    "build/src/**/*"
  ],
  "bin": {
    "export-tokens": "./export-tokens.js",
    "write-local-npmrc": "./write-local-npmrc.js"
  },
  "scripts": {
    "atest": "jest ${0}",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "gts clean",
    "fix": "gts fix",
    "lint": "gts lint",
    "publish": "yarn npm publish",
    "publish:dev": "yarn npm publish --tag dev",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "version": "yarn version",
    "version:dev": "yarn version prerelease",
    "prepack": "yarn run clean && yarn run build",
    "pretest": "yarn run build",
    "posttest": "yarn run lint"
  },
  "keywords": [
    "auth",
    "token"
  ],
  "author": "Engineering 11",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/e11community/token-exporter/issues"
  },
  "repository": {
    "url": "https://github.com/e11community/token-exporter",
    "type": "git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/e11community"
  },
  "homepage": "https://github.com/e11community/token-exporter#readme",
  "devDependencies": {
    "gts": "^3.1.0",
    "ts-node": "^10.1.0"
  },
  "dependencies": {
    "@types/node": "^16.4.10",
    "typescript": "^4.3.5"
  }
}
